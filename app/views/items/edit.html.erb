<h1>Edit Item</h1>
<%= form_with url: "/lists/#{list_id}/items/#{@item.id}" method: :patch, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :minimum %>
    <%= f.number_field :minimum %>

    <%= f.label :number_in_stock %>
    <%= f.number_field :number_field %>

    <%= f.label :item_type %>
    <%= f.number_field :item_type %>

    <%= f.submit %>
<% end %>